{
  "name": "claude-code-proxy",
  "version": "0.7.0",
  "main": "dist/server.js",
  "dependencies": {
    "@fastify/cors": "^9.x",
    "@fastify/multipart": "^8.x",
    "dotenv": "^16.x",
    "fastify": "^4.x",
    "file-type": "^21.0.0",
    "load-esm": "^1.0.2",
    "pino": "^8.x",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/file-type": "^10.6.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.x",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "eslint": "^8.x",
    "eslint-config-prettier": "^9.x",
    "eslint-plugin-prettier": "^5.x",
    "form-data": "^4.x",
    "husky": "^8.x",
    "jest": "^30.0.0",
    "lint-staged": "^15.x",
    "node-fetch": "^3.x",
    "nodemon": "^3.x",
    "pino-pretty": "^10.x",
    "prettier": "^3.x",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.x",
    "typescript": "^5.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "npm run build && nodemon --exec ts-node src/server.ts",
    "dev:build": "tsc --watch",
    "start:bg": "npm run build && (nohup node dist/server.js > server.log 2>&1 & echo $! > server.pid)",
    "stop": "[ -f server.pid ] && kill $(cat server.pid) 2>/dev/null; rm -f server.pid",
    "status": "ps -p $(cat server.pid) > /dev/null 2>&1 && echo 'Server is running (PID: '$(cat server.pid)')' || echo 'Server is not running'",
    "logs": "tail -f server.log",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest tests/e2e.test.ts --testTimeout=30000",
    "test:e2e:openai": "jest tests/openai-client-compatibility.e2e.test.ts --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:e2e:python": "jest tests/client-integrations/python-openai.e2e.test.ts --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:e2e:nodejs": "jest tests/client-integrations/nodejs-openai.e2e.test.ts --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:e2e:files": "jest tests/files-api.e2e.test.ts --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:e2e:claude-files": "jest tests/claude-files-api.e2e.test.ts --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:e2e:all": "jest --testPathPattern='.*\\.e2e\\.test\\.ts$' --testTimeout=30000 --testPathIgnorePatterns=[]",
    "test:unit": "jest --testPathIgnorePatterns='tests/e2e.test.ts|tests/server.integration.test.ts|tests/.*\\.e2e\\.test\\.ts$|tests/client-integrations/.*\\.e2e\\.test\\.ts$'",
    "test:ci": "npm run test:unit && npm run test:coverage",
    "ci:full": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage && npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
